{
  "name": "@goab/roastr",
  "version": "1.0.4",
  "description": "A hilarious console debugging roaster that turns every console.log(), error, and warning into Rick & Morty roasts for developers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
   "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "error",
    "debug",
    "roast",
    "rick-morty",
    "console",
    "fetch",
    "logging"
  ],
  "author": "Ajogu Joseph",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
